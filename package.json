{
  "name": "crossdirstat",
  "productName": "Crossdirstat",
  "version": "2.2.0-dev",
  "description": "Free open-source cross-platform file & directory statistics",
  "main": "app/index.js",
  "scripts": {
    "start": "electron app",
    "build": "node build.js",
    "buildall": "node build.js --linux --win --mac",
    "buildlinux": "node build.js --linux",
    "buildwin": "node build.js --win",
    "buildmac": "node build.js --mac",
    "fix": "eslint --fix app .eslintrc.js build.js",
    "lint": "eslint app .eslintrc.js build.js"
  },
  "repository": "https://github.com/jelmerro/crossdirstat",
  "homepage": "https://github.com/jelmerro/crossdirstat",
  "keywords": [
    "Electron",
    "Directory",
    "Statistics"
  ],
  "author": "Jelmer van Arnhem",
  "email": "Jelmerro@users.noreply.github.com",
  "license": "MIT",
  "devDependencies": {
    "electron": "18.1.0",
    "electron-builder": "23.0.3",
    "eslint": "8.14.0"
  },
  "dependencies": {
    "squarify": "1.1.0"
  }
}
